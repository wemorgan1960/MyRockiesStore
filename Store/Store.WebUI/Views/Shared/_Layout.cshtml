<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>Ask Your Mechanic Don</title>
    <!-- Bootstrap -->
    <link href="~/Theme/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!--plugins-->
    <link href="~/Theme/css/animate.css" rel="stylesheet">
    <link href="~/Theme/css/flexslider.css" rel="stylesheet">
    <link href="~/Theme/owl-carousel/assets/owl.carousel.css" rel="stylesheet">
    <link href="~/Theme/owl-carousel/assets/owl.theme.default.css" rel="stylesheet">
    <link href="~/Theme/font-awesome/css/font-awesome.css" rel="stylesheet">
    <link href="~/Theme/themify-icons/themify-icons.css" rel="stylesheet">
    <link href="~/Theme/css/jasny-bootstrap.min.css" rel="stylesheet">
    <!--Revolution slider css-->
    <link href="~/Theme/rs-plugin/css/settings.css" rel="stylesheet" type="text/css" media="screen">
    <link href="~/Theme/css/rev-style.css" rel="stylesheet" type="text/css" media="screen">
    <!--cube css-->
    <link href="~/Theme/cubeportfolio/css/cubeportfolio.min.css" rel="stylesheet" type="text/css">
    <!--custom css (comile from one-page.scss)-->
    <link href="~/Theme/css/one-page-dark.css" rel="stylesheet">
    @*<link href="css/one-page.css" rel="stylesheet">*@
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    @if (!ViewBag.IsIndexHome)
    {
        @Html.Partial("_HeaderToolBar")
    }

    @RenderBody()

    @if (!ViewBag.IsIndexHome)
    {
        @Html.Partial("_FooterToolBar")
    }
    <div id="back-to-top">
        <a href="#top"><i class="fa fa-angle-up"></i></a>
    </div>
    <!-- jQuery-->
    <script src="~/Theme/js/jquery.min.js"></script>
    <script src="~/Theme/js/jquery-migrate.min.js"></script>
    <script src="~/Theme/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Theme/js/jquery.easing.1.3.min.js" type="text/javascript"></script>
    <script src="~/Theme/js/wow.min.js" type="text/javascript"></script>
    <script src="~/Theme/js/jquery.sticky.js" type="text/javascript"></script>
    <script src="~/Theme/js/waypoints.min.js" type="text/javascript"></script>
    <script src="~/Theme/js/jquery.counterup.min.js" type="text/javascript"></script>
    <script src="~/Theme/js/modernizr.custom.97074.js" type="text/javascript"></script>
    <script src="~/Theme/js/parallax.min.js" type="text/javascript"></script>
    <script src="~/Theme/js/jquery.flexslider-min.js" type="text/javascript"></script>
    <script src="~/Theme/owl-carousel/owl.carousel.min.js" type="text/javascript"></script>
    <script src="~/Theme/js/one-page.js" type="text/javascript"></script>
    <script src="~/Theme/rs-plugin/js/jquery.themepunch.tools.min.js" type="text/javascript"></script>
    <script src="~/Theme/rs-plugin/js/jquery.themepunch.revolution.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/Theme/js/revolution-custom.js"></script>
    @*@Scripts.Render("~/bundles/scripts")*@
    @RenderSection("scripts", required: false)
</body>
</html>



